            // (2.80 (0 0 0.125))
            // (3.80 (0 0 0.125))
            // (3.90 (0 0 0.150))
            // (4.90 (0 0 0.150))
            // (5.00 (0 0 0.175))
            // (6.00 (0 0 0.175))
            // (6.10 (0 0 0.200))
            // (7.10 (0 0 0.200))
            // (7.20 (0 0 0.175))
            // (8.20 (0 0 0.175))
            // (8.30 (0 0 0.15))
            // (9.30 (0 0 0.15))
            // (9.40 (0 0 0.125))
            // (10.40 (0 0 0.125))
            // (10.50 (0 0 0.100))
            // (11.50 (0 0 0.100))
            // (11.60 (0 0 0.050))
            // (12.60 (0 0 0.050))
            // (12.70 (0 0 0.000))
            // (13.70 (0 0 0.000))
            (0.00 (0 0 0.00))
            (0.50 (0 0 0.00))
            (0.60 (0 0 0.01))
            (1.60 (0 0 0.01))
            (1.70 (0 0 0.02))
            (2.70 (0 0 0.02))
            (2.80 (0 0 0.03))
            (3.80 (0 0 0.03))
            (3.90 (0 0 0.04))
            (4.90 (0 0 0.04))
            (5.00 (0 0 0.05))
            (6.00 (0 0 0.05))
            (6.10 (0 0 0.04))
            (7.10 (0 0 0.04))
            (7.20 (0 0 0.03))
            (8.20 (0 0 0.03))
            (8.30 (0 0 0.02))
            (9.30 (0 0 0.02))
            (9.40 (0 0 0.01))
            (10.40 (0 0 0.01))
            (10.50 (0 0 0.00))
            (11.50 (0 0 0.00))
        );
    }
    outlet
    {
        type            zeroGradient;
    }
    wall
    {
        type            noSlip;
    }
    tube
    {
        type            noSlip;
    }
}


