FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1.28;

boundaryField
{
    nozzle
    {
        type            zeroGradient;
    }
   /* inlet_inner
    {
        type            zeroGradient;
    }
    inlet_outer
    {
        type            zeroGradient;
    }*/
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    wall
    {
        type            zeroGradient;
    }
    tube
    {
        type            zeroGradient;
    }
}


